<%= simple_form_for @user_info, :remote => true  do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :name %>
    <%= f.input :province,label:false,collection: [],prompt:"请选择省份" ,class:"we"%>
    <%= f.input :city,label:false,collection: [],prompt:"请选择省份" %>
    <%= f.input :county,label:false,collection: [],prompt:"请选择省份" %>
    <%= f.input :tel %>
    <%= f.input :address %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
