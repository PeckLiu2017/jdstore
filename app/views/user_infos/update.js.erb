<% html = j( render :partial => "user_infos/address_item",:locals => { :user_info => @user_info } ) %>
$("#address-item-<%=@user_info.id%>").replaceWith("<%= html %>")

$('.name-section').children('span').text(' ')

<% if @user_info.is_default == true %>
  $('#address-item-<%=@user_info.id%>').children('.name-section').children('span').text(' (默认地址)')
<% end %>
